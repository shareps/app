{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "adam-paterson/oauth2-slack": "^1.1",
        "api-platform/api-pack": "^1.1",
        "csa/guzzle-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.6",
        "jms/serializer-bundle": "^3.4",
        "jolicode/slack-php-api": "^1.2",
        "knpuniversity/oauth2-client-bundle": "^1.28",
        "league/oauth2-google": "^3.0",
        "myclabs/php-enum": "^1.7",
        "pascaldevink/shortuuid": "^2.1",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.16",
        "sensio/framework-extra-bundle": "^5.4",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/intl": "4.3.*",
        "symfony/messenger": "4.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/stopwatch": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.1",
        "fzaninotto/faker": "^1.8",
        "infection/infection": "^0.13.6",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "compile": [
            "@migrations-migrate"
        ],
        "php-cs-fixer": "./vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "migrations-migrate": "./bin/console doctrine:migrations:migrate --no-interaction",
        "migrations-add-all": "./bin/console doctrine:migrations:version --add --all",
        "schema-create": "./bin/console doctrine:schema:create",
        "phpunit": "./bin/phpunit --stop-on-failure"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
